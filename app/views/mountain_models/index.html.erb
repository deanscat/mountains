<p id="notice"><%= notice %></p>

<h1>Mountains</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mountain_models.each do |mountain_model| %>
      <tr>
        <td><%= link_to mountain_model.name, mountain_model %></td>
        <td><%= link_to 'Edit', edit_mountain_model_path(mountain_model) %></td>
        <td><%= link_to 'Destroy', mountain_model, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mountain Model', new_mountain_model_path %>
